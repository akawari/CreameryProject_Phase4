<h1>Login</h1>

<%= form_for(:session, url: sessions_path) do |form| %>
    <div class="input-field">
        <%= form.label :email %>
        <%= form.email_field :email %>
    </div>
    
    <div class="input-field">
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>
    
    <div class="actions">
        <%= form.submit "Login",:class=>"btn waves-effect waves-light" %>
  </div>
  
<% end %>
    